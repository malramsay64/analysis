#include "vect.h"

using namespace std;

int main(){
    vect v1 = vect(1,1);
    vect v2 = vect(2,2);

    vect v3 = vect(0.1,0.0);
    vect v4 = vect(0.9,0.9);
    cout << "v1+v2 " << v1+v2 << endl;
    cout << "v1-v2 " << v1-v2 << endl;
    cout << "v1*v2 " << v1*v2 << endl;
    cout << "v1/v2 " << v1/v2 << endl;
    cout << "v1*2  " << v1*2 << endl;
    cout << "v1+2  " << v1+2 << endl;
    cout << "v1-2  " << v1-2 << endl;
    cout << "v1/2  " << v1/2 << endl;
    vect v = direction(v4,v3);
    cout << "Direction v3-v4 " << v<< endl; 
    cout << " Angle v3 v4 " << atan2(direction(v4,v3))*180/PI << endl;
}
